<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">

    <title>Code Maze</title>

    <meta name="description" content="ZaneVN">
    <meta name="author" content="khanhnd">
    <meta name="robots" content="noindex, nofollow">

    <!-- Open Graph Meta -->
    <meta property="og:title" content="ZaneVN">
    <meta property="og:site_name" content="ZaneVN">
    <meta property="og:description" content="ZaneVN">
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://codemaze.net">
    <meta property="og:image" content="">

    <!-- Icons -->
    <link rel="shortcut icon" href="assets/media/favicons/favicon.png">
    <link rel="icon" type="image/png" sizes="192x192" href="assets/media/favicons/favicon-192x192.png">
    <link rel="apple-touch-icon" sizes="180x180" href="assets/media/favicons/apple-touch-icon-180x180.png">
    <!-- END Icons -->
    <!-- Stylesheets -->
    @await RenderSectionAsync("styles", false)
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&display=swap">
    <link rel="stylesheet" href="~/admin/theme/theme.bundle.min.css">
</head>
<body>

    <div id="page-container" class="sidebar-o sidebar-dark enable-page-overlay side-scroll page-header-fixed page-header-dark main-content-narrow">

        <partial name="AdminLayouts/SidebarView" />

        <partial name="AdminLayouts/HeaderView" />
        <!-- Main Container -->
        <main id="main-container">
            <div class="content content-full">
                <!-- Page Content -->
                @RenderBody()

            </div><!-- END Page Content -->
        </main>
        <!-- END Main Container -->
        <partial name="AdminLayouts/FooterView" />

    </div>
    <!-- END Page Container -->

    <script src="~/admin/js/app.min.js"></script>
    <script src="~/admin/js/jquery.min.js"></script>
    <script src="~/admin/js/magnific-popup/jquery.magnific-popup.min.js"></script>
    <script src="~/admin/js/sweetalert2/sweetalert2.min.js"></script>

    @await RenderSectionAsync("scripts", false)

    <script>ZaneVN.helpersOnLoad(['jq-magnific-popup'])</script>

</body>
</html>
